public class TestJosa {
	
	public static void main(String[] args) {
		JosaFormatter josaFormatter= new JosaFormatter();
		String text = josaFormatter.format("%s을awdw 구매하시겠습니까?", "Office");
		System.out.println(text);
	}
}
